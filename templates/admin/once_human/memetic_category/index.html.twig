{% extends 'base.html.twig' %}

{% block title %}MemeticCategory index{% endblock %}

{% block body %}
    <h1>MemeticCategory index</h1>

    <a href="{{ path('app_admin_once_human_memetic_category_new') }}" class="btn btn-primary">Create new</a>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for memetic_category in memetic_categories %}
            <tr>
                <td>{{ memetic_category.id }}</td>
                <td>{{ memetic_category.name }}</td>
                <td>
                    <a href="{{ path('app_admin_once_human_memetic_category_show', {'id': memetic_category.id}) }}">show</a>
                    <a href="{{ path('app_admin_once_human_memetic_category_edit', {'id': memetic_category.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    {{ knp_pagination_render(memetic_categories) }}
{% endblock %}
